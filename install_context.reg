@REM [HKEY_CLASSES_ROOT\*\shell\VDLL]
@REM "MUIVerb"="VDLL Convert"
@REM "SubCommands"="VDLL.toMP4;VDLL.toAVI;VDLL.toMOV;VDLL.toMKV;VDLL.toGIF;VDLL.toMP3;VDLL.toWEBM"

[HKEY_CLASSES_ROOT\SystemFileAssociations\.mp4\shell\VDLL]
