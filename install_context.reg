[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\VDLL.toWEBM]
@="Convert to .WEBM"

[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Explorer\CommandStore\shell\VDLL.toMP4\command]
@="\"C:\\Program Files\\Nitaki\\VDLL\\vdll.exe\" mp4 %1"
